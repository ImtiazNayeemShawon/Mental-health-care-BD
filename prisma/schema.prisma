datasource db {
  provider = "mongodb"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model post {
  id           String  @id @default(auto()) @map("_id") @db.ObjectId
  title        String
  description  String
  image       String
  createAt   DateTime @default(now())

}

